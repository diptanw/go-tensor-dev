{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "args": [
                "-url",
                "udp://@:9988"
            ],
            "env": {
                "CGO_CFLAGS": "-I/usr/local/include/tensorflow",
                "CGO_LDFLAGS": "-L/usr/local/lib"
            }
        }
    ]
}